config interface 'loopback'
	option proto 'static'
	option netmask '255.0.0.0'
	option device 'lo'

config interface '3g'
	option proto '3g'
	option device '/dev/ttyUSB3'
	option service 'umts'
	option ipv6 'auto'

config interface 'lan1'
	option ifname 'eth0'
	option proto 'dhcp'

config interface 'lan2'
	option proto 'static'
	option ifname 'eth1'
	option ipaddr '192.168.254.88'
	option netmask '255.255.255.0'

config interface 'lan3'
	option ifname 'eth2'
	option _orig_ifname 'eth2'
	option _orig_bridge 'false'
	option proto 'dhcp'
